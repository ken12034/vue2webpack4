{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
      "outDir": "./built/",
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "module": "es2015",
      "moduleResolution": "node",
      "target": "es5",
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
    
  },
  "include": [
      "./ROG/src/**/*"
  ]
}